<Bindings>
    <Binding name="ANSSNIPEBUYSELECT" runOnUp="true" header="[Auction Snipe]" category="AnS">
        if (keystate == "up") then
            AuctionSnipe:BuySelected();
        end
    </Binding>
    <Binding name="ANSSNIPEBUYFIRST" runOnUp="true" category="AnS">
        if (keystate == "up") then
            AuctionSnipe:BuyFirst();
        end
    </Binding>
</Bindings>