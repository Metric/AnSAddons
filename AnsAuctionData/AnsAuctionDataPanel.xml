<Ui xmlns="http://www.blizzard.com/wow/ui/">
    <Frame name="AnsAuctionDataPanelTemplate" virtual="true" inherits="TooltipBorderedFrameTemplate">
        <size x="300" y="32" />
        <Anchors>
            <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="0" y="-12"/>
        </Anchors>
        <Frames>
            <Frame name="$parentBottomBar">
                <Size x="300" y="22" />
                <Anchors>
                    <Anchor point="LEFT" relativePoint="LEFT" x="0" y="0" />
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <FontString name="$parentStatus" text="" justifyH="CENTER" inherits="GameFontHighlightSmall">
                            <Anchors>
                                <Anchor point="RIGHT" relativePoint="RIGHT" x="-10" y="0" />
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Frames>
                    <Button name="$parentStart" text="Start Data Scan" inherits="UIPanelButtonTemplate">
                        <Size x="115" y="22" />
                        <Anchors>
                            <Anchor point="LEFT" relativePoint="LEFT" x="5" y="0"/>
                        </Anchors>
                    </Button>
                </Frames>
            </Frame>
        </Frames>
    </Frame>
</Ui>