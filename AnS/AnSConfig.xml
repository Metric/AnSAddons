                <Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT">
                  <Offset>
                    <AbsDimension x="0" y="15" />
                  </Offset>
                </Anchor>
              </Anchors>
            </FontString>
          </Layer>
        </Layers>
        <Scripts>
          <OnEnterPressed>
            AnsConfig:ImportGroups(self);
