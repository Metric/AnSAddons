<Ui xmlns="http://www.blizzard.com/wow/ui/">
    <Frame name="AnsCustomSourceRowTemplate" virtual="true">
        <Size x="525" y="24" />
        <Frames>
            <EditBox parentKey="Name" name="$parentName" autoFocus="false" inherits="AnsEditBoxTemplate">
                <Size x="100" y="24" />
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="0" />
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString inherits="AnsGameFontWhiteTrans" justifyH="LEFT" text="Name">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT" x="0" y="5"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </EditBox>
            <EditBox parentKey="Value" name="$parentValue" autoFocus="false" inherits="AnsEditBoxTemplate">
                <Size x="200" y="24" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parentName" relativePoint="TOPRIGHT" x="10" y="0" />
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString inherits="AnsGameFontWhiteTrans" justifyH="LEFT" text="Value">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT" x="0" y="5"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </EditBox>
            <Button parentKey="Delete" name="$parentDelete" inherits="AnsIconDeleteTemplate">
                <Size x="16" y="16" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parentValue" relativePoint="TOPRIGHT" x="10" y="-4" />
                </Anchors>
            </Button>
        </Frames>
    </Frame>

    <Frame name="AnsCustomSourcesTemplate" virtual="true">
        <Size x="525" y="345" />
        <Anchors>
            <Anchor point="TOPLEFT" x="168" y="-32" />
        </Anchors>
        <Frames>
            <Button parentKey="NewSource" text="Add Source" name="$parentNewSource" inherits="AnsStandardButtonTemplate">
                <Size x="100" y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="32" />
                </Anchors>
            </Button>
            <ScrollFrame parentKey="ScrollFrame" name="$parentListView" inherits="AnsFauxScrollFrameTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="0" />
                </Anchors>
                <Size x="525" y="340" />
            </ScrollFrame>
        </Frames>
    </Frame>

    <Frame name="AnsMacroSettingsTemplate" virtual="true">
        <Size x="525" y="345" />
         <Anchors>
            <Anchor point="TOPLEFT" x="168" y="-32" />
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <FontString inherits="AnsGameFontWhiteTrans" justifyH="LEFT" text="Create a macro bound to the scroll wheel to activate actions">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="0"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <CheckButton parentKey="tsmCancel" text="TSM Cancel Auction" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-25" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="tsmPost" text="TSM Post Auction" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0" relativePoint="TOPRIGHT" relativeKey="$parent.tsmCancel" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="tsmBuy" text="TSM Buy Auction" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.tsmPost" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="tsmSniper" text="TSM Sniper Buy" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.tsmBuy" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="tsmCrafting" text="TSM Crafting" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-10"  relativePoint="BOTTOMLEFT" relativeKey="$parent.tsmCancel" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="tsmDestroying" text="TSM Destroy" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.tsmCrafting" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="tsmVendoring" text="TSM Vendor All" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.tsmDestroying" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="ansPost" text="AnS Post Auction" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-25" relativePoint="BOTTOMLEFT" relativeKey="$parent.tsmCrafting" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="ansCancel" text="AnS Cancel Auction" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.ansPost" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="ansSniper" text="AnS Sniper Buy" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.ansCancel" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="ansSniperFirst" text="AnS Sniper Buy First" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.ansSniper" />
                </Anchors>
            </CheckButton>

            <CheckButton parentKey="up" text="Mouse Wheel Up" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-25"  relativePoint="BOTTOMLEFT" relativeKey="$parent.ansPost" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="down" text="Mouse Wheel Down" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.up" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="ctrl" text="CTRL Modifier" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.down" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="shift" text="SHIFT Modifier" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="120" y="0"  relativePoint="TOPRIGHT" relativeKey="$parent.ctrl" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="alt" text="ALT Modifier" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-25"  relativePoint="BOTTOMLEFT" relativeKey="$parent.up" />
                </Anchors>
            </CheckButton>

            <Button parentKey="CreateMacroBtn" text="Create Macro" inherits="AnsStandardButtonTemplate">
                <Size x="525" y="32" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.alt" relativePoint="BOTTOMLEFT" x="0" y="-25" />
                </Anchors>
            </Button>
        </Frames>
    </Frame>

    <Frame name="AnsGeneralSettingsTemplate" virtual="true">
         <Size x="525" y="345" />
        <Anchors>
            <Anchor point="TOPLEFT" x="168" y="-32" />
        </Anchors>
        <Frames>
            <CheckButton parentKey="DressUp" text="Show Dressup" name="$parentDressUp" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="0" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="Coins" text="Use Coin Icons" name="$parentCoins" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="200" y="0" relativePoint="TOPRIGHT" relativeTo="$parentDressUp" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="Analytics" text="Track Gold, Auctions etc." name="$parentAnalytics" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="200" y="0"  relativePoint="TOPRIGHT" relativeTo="$parentCoins" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RRTooltip" text="Show Realm Recent Tooltip" name="$parentRRTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-20"  relativePoint="BOTTOMLEFT" relativeTo="$parentDressUp" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RMTooltip" text="Show Realm Min Tooltip" name="$parentRMTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="200" y="0"  relativePoint="TOPRIGHT" relativeTo="$parentRRTooltip" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RThreeTooltip" text="Show Realm 3-Day Tooltip" name="$parentRThreeTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="200" y="0"  relativePoint="TOPRIGHT" relativeTo="$parentRMTooltip" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RMMTooltip" text="Show Realm Market Tooltip" name="$parentRMMTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-10"  relativePoint="BOTTOMLEFT" relativeTo="$parentRRTooltip" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RRRTooltip" text="Show Region Recent Tooltip" name="$parentRRRTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-20"  relativePoint="BOTTOMLEFT" relativeTo="$parentRMMTooltip" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RRMTooltip" text="Show Region Min Tooltip" name="$parentRRMTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="200" y="0"  relativePoint="TOPRIGHT" relativeTo="$parentRRRTooltip" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RRThreeTooltip" text="Show Region 3-Day Tooltip" name="$parentRRThreeTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="200" y="0"  relativePoint="TOPRIGHT" relativeTo="$parentRRMTooltip" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="RRMMTooltip" text="Show Region Market Tooltip" name="$parentRRMMTooltip" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-10"  relativePoint="BOTTOMLEFT" relativeTo="$parentRRRTooltip" />
                </Anchors>
            </CheckButton>

            <CheckButton parentKey="SaveWindowLocations" text="Remember Window Locations" name="$parentSaveWindowLocations" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-20"  relativePoint="BOTTOMLEFT" relativeTo="$parentRRMMTooltip" />
                </Anchors>
            </CheckButton>

            <CheckButton parentKey="ShowDebug" text="Show Debug Window" name="$parentShowDebug" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" x="-100" y="10" />
                </Anchors>
            </CheckButton>
        </Frames>
    </Frame>

    <Frame name="AnsCraftSettingsTemplate" virtual="true">
        <Size x="525" y="345" />
        <Anchors>
            <Anchor point="TOPLEFT" x="168" y="-32" />
        </Anchors>
         <Layers>
            <Layer level="ARTWORK">
                <FontString justifyH="LEFT" text="Craft Value" name="$parentValueLabel" inherits="AnsGameFontWhiteTrans">
                    <Size x="100" y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="0" />
                    </Anchors>
                </FontString>
                <FontString justifyH="LEFT" text="Material Cost" justifyV="CENTER" name="$parentMCostLabel" inherits="AnsGameFontWhiteTrans">
                    <Size x="200" y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="-45" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <EditBox parentKey="CraftValue" text="" name="$parentCraftValue" inherits="AnsEditBoxTemplate">
                <Size x="525" y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentValueLabel" x="0" y="0" />
                </Anchors>
            </EditBox>
            <EditBox parentKey="MaterialCost" text="" name="$parentMaterialCost" inherits="AnsEditBoxTemplate">
                <Size x="525" y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentMCostLabel" x="0" y="0" />
                </Anchors>
            </EditBox>
        </Frames>
    </Frame>

    <Frame name="AnsSniperSettingsTemplate" virtual="true">
        <Size x="525" y="345" />
        <Anchors>
            <Anchor point="TOPLEFT" x="168" y="-32" />
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <FontString justifyH="LEFT" text="Price Source" name="$parentSourceLabel" inherits="AnsGameFontWhiteTrans">
                    <Size x="100" y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="0" />
                    </Anchors>
                </FontString>
                <FontString justifyH="LEFT" text="Default Max Price or Boolean Filter" justifyV="CENTER" name="$parentPriceLabel" inherits="AnsGameFontWhiteTrans">
                    <Size x="200" y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="-45" />
                    </Anchors>
                </FontString>
                <FontString justifyH="LEFT" justifyV="CENTER" text="Character Blacklist (one entry per line)" name="$parentBlacklistLabel" inherits="AnsGameFontWhiteTrans">
                    <Size x="200" y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="-194" />
                    </Anchors>
                </FontString>
                <FontString justifyH="LEFT" text="Ding Sound" inherits="AnsGameFontWhiteTrans">
                    <Size x="100" y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="225" y="0" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <EditBox parentKey="Source" text="" name="$parentSourceValue" inherits="AnsEditBoxTemplate">
                <Size x="200" y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentSourceLabel" x="0" y="0" />
                </Anchors>
            </EditBox>
            <ScrollFrame parentKey="Price" enableMouse="true" name="$parentPriceValue" inherits="AnsScrollFrameTemplate">
                <Size x="575" y="128" />
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="0" relativePoint="BOTTOMLEFT" relativeTo="$parentPriceLabel" />
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture>
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0" />
                                <Anchor point="BOTTOMRIGHT" x="0" y="0" />
                            </Anchors>
                            <Color r="1" g="1" b="1" a="0.05" />
                        </Texture>
                    </Layer>
                </Layers>
                <ScrollChild>
                    <EditBox parentKey="Text" name="$parentText" inherits="AnsEditBoxTemplate">
                        <Size x="575" y="123" />
                    </EditBox>
                </ScrollChild>
                <Scripts>
                    <OnLoad>
                        self:SetScript("OnMouseDown", function() self.Text:SetFocus() end);
                    </OnLoad>
                </Scripts>
            </ScrollFrame>
            <ScrollFrame parentKey="Blacklist" enableMouse="true" name="$parentBlacklist" inherits="AnsScrollFrameTemplate">
                <Size x="200" y="128" />
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="0" relativePoint="BOTTOMLEFT" relativeTo="$parentBlacklistLabel" />
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture>
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0" />
                                <Anchor point="BOTTOMRIGHT" x="0" y="0" />
                            </Anchors>
                            <Color r="1" g="1" b="1" a="0.05" />
                        </Texture>
                    </Layer>
                </Layers>
                <ScrollChild>
                    <EditBox parentKey="Text" name="$parentText" inherits="AnsEditBoxTemplate">
                        <Size x="200" y="123" />
                    </EditBox>
                </ScrollChild>
                <Scripts>
                    <OnLoad>
                        self:SetScript("OnMouseDown", function() self.Text:SetFocus() end);
                    </OnLoad>
                </Scripts>
            </ScrollFrame>
            <CheckButton parentKey="CommodityConfirm" text="Use Commodity Confirm" name="$parentCommodityConfirm" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$parentBlacklistLabel" x="20" y="-15" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="Ding" text="Use Ding Sound" name="$parentDing" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentCommodityConfirm" x="0" y="-10" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="SkipSeenGroup" text="Skip Lowest Seen Group" name="$parentSkipSeenGroup" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$parentCommodityConfirm" x="150" y="0" />
                </Anchors>
            </CheckButton>
            <CheckButton parentKey="ChatMessageNew" text="Chat Message New (Classic)" name="$parentChatMessageNew" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentSkipSeenGroup" x="0" y="-10" />
                </Anchors>
            </CheckButton>
            <EditBox parentKey="ItemsUpdate" name="$parentItemsUpdate" text="" numeric="true" inherits="AnsEditBoxTemplate">
                <Size x="64" y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentDing" x="0" y="-30" />
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString justifyH="LEFT" text="Items Processed Per Frame" inherits="AnsGameFontWhiteTrans">
                            <Size x="150" y="20" />
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="20" />
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </EditBox>
            <EditBox parentKey="ScanDelay" name="$parentScanDelay" text="" numeric="true" inherits="AnsEditBoxTemplate">
                <Size x="64" y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentItemsUpdate" x="0" y="-20" />
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString justifyH="LEFT" text="Found Scan Delay" inherits="AnsGameFontWhiteTrans">
                            <Size x="200" y="20" />
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="20" />
                            </Anchors>
                        </FontString>
                        <FontString justifyH="LEFT" text="seconds" inherits="AnsGameFontWhiteTrans">
                            <Anchors>
                                <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parent" x="5" y="0" />
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </EditBox>
            <CheckButton parentKey="FlashWoWIcon" text="Flash WoW Icon" name="$parentFlashWoWIcon" inherits="AnsCheckboxTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentChatMessageNew" x="0" y="-10" />
                </Anchors>
            </CheckButton>
        </Frames>
    </Frame>
</Ui>