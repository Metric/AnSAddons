<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Button name="AnsAuctionRowTemplate" virtual="true" hidden="true">
    <Size x="594" y="16" />
    <Layers>
      <Layer level="BACKGROUND">
        <Texture hidden="true" parentKey="NewHighlight" name="$parentNewHighlight" alphaMode="ADD" file="Interface\Buttons\WHITE8X8" horizTile="true" vertTile="true">
          <Color r="1" g="1" b="1" a="0.05" />
        </Texture>
      </Layer>
    </Layers>
    <Frames>
      <Frame>
        <Size x="140" y="16" />
        <Anchors>
          <Anchor point="LEFT" x="0" y="0"/>
        </Anchors>
        <Layers>
          <Layer level="ARTWORK">
            <FontString name="$parentPPU" justifyH="RIGHT" inherits="AnsAuctionFont">
              <Size x="140" y="16" />
              <Anchors>
                <Anchor point="LEFT" x="0" y="0" />
              </Anchors>
            </FontString>
          </Layer>
        </Layers>
      </Frame>

      <Frame>
        <Size x="16" y="16" />
        <Anchors>
          <Anchor point="LEFT" relativeTo="$parentPPU" relativePoint="RIGHT" x="2" y="0" />
        </Anchors>
        <Layers>
          <Layer level="ARTWORK">
            <Texture name="$parentItemIcon">
                <Size x="16" y="16"/>
                <TexCoords left="0" right="1" top="0" bottom="1"/>
            </Texture>
          </Layer>
        </Layers>
      </Frame>

      <Frame>
        <Size x="210" y="16" />
        <Anchors>
          <Anchor point="LEFT" relativeTo="$parentItemIcon" relativePoint="RIGHT" x="2" y="0"/>
        </Anchors>
        <Layers>
          <Layer level="BACKGROUND">
            <FontString name="$parentNameText" inherits="AnsAuctionFont" justifyH="LEFT">
              <Size x="210" y="16"/>
            </FontString>
          </Layer>
        </Layers>
      </Frame>

      <Frame>
        <Size x="40" y="16" />
        <Anchors>
          <Anchor point="LEFT" relativeTo="$parentNameText" relativePoint="RIGHT" x="10" y="0"/>
        </Anchors>
        <Layers>
          <Layer level="BACKGROUND">
            <FontString name="$parentPercent" inherits="AnsAuctionFont" justifyH="LEFT">
              <Size x="40" y="16" />
            </FontString>
          </Layer>
        </Layers>
      </Frame>

      <Frame>
        <Size x="50" y="16" />
        <Anchors>
          <Anchor point="LEFT" relativeTo="$parentPercent" relativePoint="RIGHT" x="40" y="0"/>
        </Anchors>
        <Layers>
          <Layer level="BACKGROUND">
            <FontString name="$parentILevel" inherits="AnsAuctionFont" justifyH="LEFT">
              <Size x="32" y="16" />
            </FontString>
          </Layer>
        </Layers>
      </Frame>

      <Frame>
        <Size x="100" y="16" />
        <Anchors>
          <Anchor point="LEFT" relativeTo="$parentILevel" relativePoint="RIGHT" x="10" y="0"/>
        </Anchors>
        <Layers>
          <Layer level="BACKGROUND">
            <FontString name="$parentOwner" inherits="AnsAuctionFont" justifyH="LEFT"/>
          </Layer>
        </Layers>
      </Frame>
    </Frames>

    <PushedTexture file="Interface\HelpFrame\HelpFrameButton-Highlight">
      <TexCoords left="0.25" right="0.35" top="0.0" bottom="0.6"/>
    </PushedTexture>
    <HighlightTexture file="Interface\HelpFrame\HelpFrameButton-Highlight" alphaMode="ADD">
      <TexCoords left="0.035" right="0.04" top="0.2" bottom="0.25"/>
    </HighlightTexture>
  </Button>
</Ui>