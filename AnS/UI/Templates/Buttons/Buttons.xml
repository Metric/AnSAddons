     <Button name="AnsTreeRowTemplate" inherits="AnsTab2Template" virtual="true">
        <Size x="200" y="20"/>
		<Frames>
			<Button name="$parentExpander" text="+" inherits="AnsStandardButtonTemplate">
				<Size x="12" y="12" />
				<Anchors>
					<Anchor point="RIGHT" x="-2" y="0" relativeTo="$parentText" relativePoint="LEFT" />
				</Anchors>
			</Button>
            <Button name="$parentMoveDown" inherits="AnsDownTemplate">
                <Size x="12" y="12" />
                <Anchors>
                    <Anchor point="RIGHT" />
                </Anchors>
            </Button>
            <Button name="$parentMoveUp" inherits="AnsUpTemplate">
                <Size x="12" y="12" />
                <Anchors>
                    <Anchor point="RIGHT" relativeTo="$parentMoveDown" relativePoint="LEFT" x="0" y="0" />
                </Anchors>
            </Button>
            <Button name="$parentAdd" inherits="AnsIconAddtemplate">
                <Size x="16" y="16" />
                <Anchors>
                    <Anchor point="RIGHT" relativeTo="$parentMoveUp" relativePoint="LEFT" x="0" y="0" />
                </Anchors>
            </Button>
		</Frames>
    </Button>
    <Button Name="AnsTreeRowAddTemplate" inherits="AnsTab2Template" virtual="true">
        <Size x="200" y="20" />
        <Frames>
        	<Button name="$parentExpander" text="+" inherits="AnsStandardButtonTemplate">
				<Size x="12" y="12" />
				<Anchors>
					<Anchor point="RIGHT" x="-2" y="0" relativeTo="$parentText" relativePoint="LEFT" />
				</Anchors>
			</Button>
            <Button name="$parentAdd" inherits="AnsIconAddTemplate">
                <Size x="16" y="16" />
                <Anchors>
                    <Anchor point="RIGHT" x="-4" y="0" />
                </Anchors>
            </Button>
        </Frames>
    </Button>
    <Button name="AnsMiniButtonTemplate" parent="Minimap" virtual="true">
        <Size x="33" y="33" />
        <Layers>
            <Layer level="ARTWORK">
                <Texture name="$parentIcon">
                    <Size x="21" y="21" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="6" y="-6" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
                <Texture file="Interface\Minimap\MiniMap-TrackingBorder">
                    <Size x="56" y="56" />
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight">
            <Size x="33" y="33 " />
            <Anchors>
                <Anchor point="TOPLEFT" x="0" y="0" />
            </Anchors>
        </HighlightTexture>
        <Scripts>
            <OnLoad>
                self:RegisterForClicks("LeftButtonUp","RightButtonUp")
                self:RegisterForDrag("LeftButton","RightButton")
            </OnLoad>
        </Scripts>
    </Button>
</Ui>