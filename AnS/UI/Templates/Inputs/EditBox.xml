<Ui xmlns="http://www.blizzard.com/wow/ui/">
    <EditBox name="AnsEditBoxTemplate" virtual="true" autoFocus="false">
        <Anchors>
            <Anchor point="TOPLEFT" x="0" y="0" />
        </Anchors>
        <FontString inherits="AnsGameFontNormalLight" justifyH="LEFT" />
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Background" file="Interface\Buttons\WHITE8X8" horizTile="true" vertTile="true">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="0" />
                        <Anchor point="BOTTOMRIGHT" x="0" y="0" />
                    </Anchors>
                    <Color r="1" g="1" b="1" a="0.05" />
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <FontString name="$parentLabel" inherits="AnsGameFontNormalWhiteTrans">
                    <Anchors>
                        <Anchor point="LEFT" x="5" y="0" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="Clear" name="$parentClear">
                <Size x="18" y="18" />
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-5" y="-2" />
                </Anchors>
                <NormalTexture file="Interface\AddOns\AnS\Images\icons">
                    <TexCoords left="0" right="0.5" top="0" bottom="0.5" />
                    <Size x="18" y="18" />
                    <Color r="0" g="0" b="0" a="0.75" />
                </NormalTexture>
                <HighlightTexture file="Interface\AddOns\AnS\Images\icons">
                    <TexCoords left="0" right="0.5" top="0" bottom="0.5" />
                    <Size x="18" y="18" />
                    <Color r="0" g="0" b="0" a="1" />
                </HighlightTexture>
            </Button>
        </Frames>
    </EditBox>
</Ui>